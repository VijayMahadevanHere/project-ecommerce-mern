

    <style>
      /* Add some style to the table */
      table {
        border-collapse: collapse;
        width: 100%;
        margin-top: 20px;
        font-family: Arial, sans-serif;
      }
      th, td {
        padding: 10px;
        text-align: left;
        border-bottom: 1px solid #ddd;
      }
      th {
        background-color: #f2f2f2;
        color: #666;
        font-size: 24px;
        text-transform: uppercase;
      }
      td {
        font-size: 20px;
      }
      /* Add some style to the page header */
      h1 {
        text-align: center;
        font-size: 48px;
        margin-top: 40px;
        font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        color: #444;
        text-shadow: 2px 2px 4px rgba(0,0,0,0.2);
      }
      /* Add some style to the body */
      body {
        background-color: #f9f9f9;
      }
    </style>
  
  <body>
    <h1>List of Coupons</h1>
    <table>
      <thead>
        <tr>
          <th>Coupon Name</th>
          <th>Coupon Price</th>
          <th>Description</th>
          <th>Starting Date</th>
          <th>Expiration Date</th>
        </tr>
      </thead>
      <tbody>
        <!-- Replace the following rows with actual data from the admin side -->
        <% response.forEach(function(response) { %>
        <tr>
          <td><%=response.Name%></td>
          <td><%=response.Price%></td>
          <td><%=response.Discription%></td>
          <td><%=response.startDate%></td>
          <td style="color: red;"><%=response.endDate%></td>     
        </tr>  
        <% }) %>
      </tbody>
    </table>
  </body>
